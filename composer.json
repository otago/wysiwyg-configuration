{
  "name": "otago/wysiwyg-configuration",
  "description": "WYSIWYG (TinyMCE) configuration for SilverStripe CMS (vendormodule).",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "University of Otago",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "silverstripe/framework": "^4.13 || ^5.1",
    "silverstripe/admin": "^1.13 || ^2"
  },
  "autoload": {
    "psr-4": {
      "Otago\\WysiwygConfiguration\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client"
    ],
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}